{
  "name": "@ianwremmel/eslint-plugin-ianwremmel",
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/eslint-plugin-ianwremmel.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "eslint": "ESLINT_USE_FLAT_CONFIG=false eslint",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:es": "npm run --silent eslint -- .",
    "semantic-release": "semantic-release",
    "sync-peer-deps": "node ./scripts/sync-peer-deps",
    "test": "npm run --silent lint",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "lint-staged": {
    "*.js": "npm run eslint -- "
  },
  "release": {
    "branches": [
      "master"
    ],
    "ci": true
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^3.0.3",
    "semantic-release": "^24.2.7",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=8.42.0",
    "@typescript-eslint/parser": ">=8.42.0",
    "eslint": ">=9.34.0",
    "eslint-config-prettier": ">=10.1.8",
    "eslint-plugin-babel": ">=5.3.1",
    "eslint-plugin-eslint-comments": ">=3.2.0",
    "eslint-plugin-import": ">=2.28.1",
    "eslint-plugin-jsx-a11y": ">=6.7.1",
    "eslint-plugin-markdown": ">=5.1.0",
    "eslint-plugin-prettier": ">=5.0.1",
    "eslint-plugin-react": ">=7.33.2",
    "eslint-plugin-react-hooks": ">=5.2.0",
    "prettier": ">=3.0.3",
    "typescript": ">=5.2.2"
  }
}
