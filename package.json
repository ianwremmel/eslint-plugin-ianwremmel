{
  "name": "@ianwremmel/eslint-plugin-ianwremmel",
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/eslint-plugin-ianwremmel.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "eslint": "eslint --ignore --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:js": "npm run --silent eslint -- .",
    "semantic-release": "semantic-release",
    "test": "npm run --silent lint"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "lint-staged": {
    "*.js": "npm run eslint -- "
  },
  "release": {
    "branches": [
      "master"
    ],
    "verifyConditions": "condition-circle"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "condition-circle": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.1"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  }
}
